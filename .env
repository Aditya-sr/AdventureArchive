APP_NAME=Laravel
APP_ENV=production  # Ensure this is 'production' in live environments
APP_KEY=base64:eR1f+/uY+HYXhQtFj1H/PcbV27U8QE4aDtyX2un5Eo4=  # Your app key
APP_DEBUG=false  # Should be false in production
APP_URL=https://adventurearchive-production.up.railway.app  # Set your actual URL here

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1  # Database host should be correctly set
DB_PORT=3306  # Database port
DB_DATABASE=adventure_archive  # Your database name
DB_USERNAME=root  # Your DB username
DB_PASSWORD=  # Your DB password, ensure this is correct in production

# Caching & Queues
CACHE_DRIVER=file  # Or 'redis' if you're using Redis
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Redis Configuration (if you're using Redis)
REDIS_HOST=127.0.0.1  # Ensure this is correct if you're using Redis
REDIS_PASSWORD=null  # Set Redis password if applicable
REDIS_PORT=6379  # Default Redis port

# Remove unnecessary configurations (e.g., Mail, Pusher, AWS)
BROADCAST_DRIVER=log  # Default broadcasting driver
FILESYSTEM_DISK=local  # Local disk for file storage

# Uncomment and update below only if using AWS services
# AWS_ACCESS_KEY_ID=your_aws_access_key_id
# AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
# AWS_DEFAULT_REGION=us-east-1
# AWS_BUCKET=your_s3_bucket_name
